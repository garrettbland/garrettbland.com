{
    "name": "garrettbland.com",
    "version": "0.1.0",
    "private": true,
    "prettier": {
        "semi": false,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 70,
        "tabWidth": 4
    },
    "scripts": {
        "start": "npm run serve",
        "clean": "rimraf dist",
        "serve:eleventy": "cross-env NODE_ENV=development npx @11ty/eleventy --serve",
        "serve:webpack": "webpack --mode development --watch",
        "serve": "npm-run-all clean --parallel serve:*",
        "build:eleventy": "cross-env NODE_ENV=production npx @11ty/eleventy",
        "build:webpack": "webpack --mode production",
        "build": "npm-run-all clean build:*"
    },
    "dependencies": {},
    "devDependencies": {
        "@11ty/eleventy": "^0.11.0",
        "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
        "autoprefixer": "^9.8.0",
        "cross-env": "^7.0.2",
        "css-loader": "^3.5.3",
        "mini-css-extract-plugin": "^0.9.0",
        "npm-run-all": "^4.1.5",
        "postcss-loader": "^3.0.0",
        "prettier": "^2.0.5",
        "prismjs": "^1.20.0",
        "rimraf": "^3.0.2",
        "tailwindcss": "^1.4.6",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    }
}
